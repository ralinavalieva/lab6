# запрашиваем у пользователя три числа
a = int(input('введите первое число: '))
b = int(input('введите второе число: '))
c = int(input('введите третье число: '))
# выводим макс и мин значение
print(max(a,b,c))
print(min(a,b,c))
# проверка с помощью if равенство чисел
if a==b==c:
  print('равны')
else:
  print("не равны")
